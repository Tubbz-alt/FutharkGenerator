{
    "name": "acwpd/futhark-generator",
    "description": "This is a PHP fork of the Futhark generator for Worm.",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "PHP": "^7.0",
        "ktamas77/firebase-php": "dev-master",
        "altorouter/altorouter": "^1.2",
        "acwpd/helpers": "@dev",
        "kreait/firebase-php": "3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/farfromunique/Helpers.git"
        }
    ],
    "authors": [
        {
            "name": "Aaron C",
            "email": "git@acwpd.com"
        }
    ],
    "config": {
        "vendor-dir": "protected/vendor"
    },
    "autoload": {
        "psr-4": {
            "ACWPD\\Futhark\\": "protected/classes/"
        }
    },
    "scripts":{
        "serve": "php -S localhost:800 -t public/"
    }
}
